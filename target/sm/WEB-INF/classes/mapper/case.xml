<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Case">

    <select id="queryCaseById" parameterType="java.lang.Integer" resultType="com.sm.model.Case">
            SELECT * FROM T_FMS_CASE WHERE id = #{id}
